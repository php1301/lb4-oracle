{
  "name": "movie-heqt",
  "version": "0.0.1",
  "description": "movie",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t movie-heqt .",
    "docker:run": "docker run -p 3000:3000 -d movie-heqt",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "start": "node -r source-map-support/register .",
    "start:watch": "tsc-watch -b --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "php1301 <19520854@gm.uit.edu.vn>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.1.0",
    "@loopback/authentication-jwt": "^0.8.0",
    "@loopback/authentication-passport": "^3.1.0",
    "@loopback/authorization": "^0.8.1",
    "@loopback/boot": "^3.3.0",
    "@loopback/core": "^2.15.0",
    "@loopback/repository": "^3.5.0",
    "@loopback/rest": "^9.2.0",
    "@loopback/rest-explorer": "^3.2.0",
    "@loopback/service-proxy": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "casbin": "^5.6.1",
    "cfenv": "^1.2.4",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "loopback-connector-cloudant": "^2.5.0",
    "loopback-connector-oracle": "^4.5.2",
    "passport-jwt": "^4.0.0",
    "tsc-watch": "^4.2.9",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.0",
    "@loopback/eslint-config": "^10.1.0",
    "@loopback/testlab": "^3.3.0",
    "@types/node": "^10.17.55",
    "@types/passport-jwt": "^3.0.5",
    "eslint": "^7.22.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.2.3"
  }
}
